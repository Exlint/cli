{
	"name": "@exlint.io/cli",
	"version": "1.3.3",
	"author": "Exlint <dev@exlint.io> (https://exlint.io)",
	"homepage": "https://github.com/Exlint/cli#readme",
	"description": "Exlint CLI - Centralize coding standards across repositories in seconds.",
	"keywords": [
		"linter",
		"best-practices",
		"centralized",
		"code-quality",
		"code-analysis-tool",
		"coding-standards"
	],
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/Exlint/cli/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Exlint/cli.git"
	},
	"main": "./dist/index.js",
	"files": [
		"./dist/main.js",
		"./bin"
	],
	"bin": {
		"exlint": "./bin/exlint.js"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"prepublishOnly": "nest build",
		"husky:install": "husky install",
		"git:config": "git config --local include.path ../.gitconfig",
		"setdev": "run-p husky:install git:config",
		"onboarding": "node ./scripts/onboarding.cjs",
		"prepare": "is-ci || run-s setdev onboarding",
		"build": "nest build",
		"build:dev": "cross-env NODE_ENV=development nest build",
		"compodoc": "compodoc && open-cli ./documentation/index.html",
		"lint": "eslint -c ./.eslintrc.cjs \"**/*.{ts,js,cjs}\"",
		"type-check": "tsc --project ./tsconfig.json",
		"inflint": "inflint -c ./inflint.config.ts",
		"depcheck": "depcheck --config ./.depcheckrc.json",
		"prettier": "prettier --check **/*.{ts,js,cjs,json,yaml}"
	},
	"dependencies": {
		"@nestjs/common": "^9.0.0",
		"@nestjs/core": "^9.0.0",
		"axios": "1.3.3",
		"chalk": "4.1.2",
		"cli-spinners": "2.7.0",
		"envinfo": "7.8.1",
		"express": "4.18.2",
		"fs-extra": "11.1.0",
		"ink": "3.2.0",
		"ink-divider": "3.0.0",
		"ink-spinner": "4.0.3",
		"ink-task-list": "2.0.0",
		"is-ci": "3.0.1",
		"nest-commander": "3.5.0",
		"netrc-parser": "3.1.6",
		"open": "8.4.1",
		"react": "18.2.0",
		"reflect-metadata": "^0.1.13",
		"rxjs": "7.8.0",
		"v8-compile-cache": "2.3.0"
	},
	"devDependencies": {
		"@commitlint/cli": "17.4.4",
		"@compodoc/compodoc": "1.1.19",
		"@exlint.io/inflint": "1.2.10",
		"@nestjs/cli": "9.2.0",
		"@types/envinfo": "7.8.1",
		"@types/express": "4.17.17",
		"@types/fs-extra": "11.0.1",
		"@types/ink-divider": "2.0.2",
		"@types/inquirer": "9.0.3",
		"@types/is-ci": "3.0.0",
		"@types/node": "18.13.0",
		"@types/react": "18.0.28",
		"@typescript-eslint/eslint-plugin": "5.52.0",
		"@typescript-eslint/parser": "5.52.0",
		"commitizen": "4.3.0",
		"cross-env": "7.0.3",
		"cz-vinyl": "1.6.2",
		"depcheck": "1.4.3",
		"eslint": "8.34.0",
		"eslint-config-prettier": "8.6.0",
		"eslint-plugin-import": "2.27.5",
		"eslint-plugin-node": "11.1.0",
		"eslint-plugin-react": "7.32.2",
		"eslint-plugin-unused-imports": "2.0.0",
		"figlet": "1.5.2",
		"husky": "8.0.3",
		"lint-staged": "13.1.2",
		"npm-run-all": "4.1.5",
		"open-cli": "7.1.0",
		"prettier": "2.8.4",
		"ts-loader": "9.4.2",
		"typescript": "4.9.5",
		"webpack": "5.75.0",
		"webpack-merge": "5.8.0"
	},
	"peerDependencies": {
		"@types/inquirer": "9.0.3"
	}
}
