{
	"name": "@exlint/cli",
	"version": "0.0.1",
	"description": "Exlint",
	"author": "Exlint <dev@exlint.io> (https://www.exlint.io)",
	"license": "MIT",
	"homepage": "https://github.com/Exlint/cli#readme",
	"bugs": {
		"url": "https://github.com/Exlint/cli/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Exlint/cli.git"
	},
	"main": "./dist/index.js",
	"files": [
		"./dist/index.js",
		"./bin"
	],
	"bin": {
		"exlint": "./bin/exlint.js"
	},
	"scripts": {
		"husky:install": "husky install",
		"git:config": "git config --local include.path ../.gitconfig",
		"setdev": "run-p husky:install git:config",
		"onboarding": "node ./scripts/onboarding.cjs",
		"prepare": "is-ci || run-s setdev onboarding",
		"build": "nest build",
		"build:dev": "cross-env NODE_ENV=development nest build",
		"compodoc": "compodoc && open-cli ./documentation/index.html",
		"lint": "eslint -c ./.eslintrc.cjs \"**/*.{ts,js,cjs}\" --fix",
		"type-check": "tsc --project ./tsconfig.json",
		"inflint": "inflint -c ./inflint.config.ts",
		"depcheck": "depcheck --config ./.depcheckrc.json",
		"prettier": "prettier --check **/*.{ts,js,cjs,json,yaml}"
	},
	"dependencies": {
		"@nestjs/common": "^9.0.0",
		"@nestjs/core": "^9.0.0",
		"axios": "0.27.2",
		"chalk": "4.1.2",
		"cli-spinners": "2.7.0",
		"envinfo": "7.8.1",
		"express": "4.18.2",
		"fs-extra": "11.1.0",
		"ink": "3.2.0",
		"ink-spinner": "4.0.3",
		"ink-task-list": "2.0.0",
		"is-ci": "3.0.1",
		"nest-commander": "3.4.0",
		"netrc-parser": "3.1.6",
		"open": "8.4.0",
		"react": "18.2.0",
		"reflect-metadata": "^0.1.13",
		"rxjs": "7.5.7",
		"v8-compile-cache": "2.3.0",
		"xml2js": "0.4.23"
	},
	"devDependencies": {
		"@commitlint/cli": "17.1.2",
		"@compodoc/compodoc": "1.1.19",
		"@exlint.io/inflint": "1.2.9",
		"@nestjs/cli": "9.1.5",
		"@types/envinfo": "7.8.1",
		"@types/express": "4.17.15",
		"@types/fs-extra": "9.0.13",
		"@types/inquirer": "9.0.3",
		"@types/is-ci": "3.0.0",
		"@types/node": "16.18.2",
		"@types/react": "18.0.26",
		"@types/xml2js": "0.4.11",
		"@typescript-eslint/eslint-plugin": "5.41.0",
		"@typescript-eslint/parser": "5.41.0",
		"commitizen": "4.2.5",
		"cross-env": "7.0.3",
		"cz-vinyl": "1.5.0",
		"depcheck": "1.4.3",
		"eslint": "8.26.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-import": "2.26.0",
		"eslint-plugin-node": "11.1.0",
		"eslint-plugin-react": "7.31.10",
		"eslint-plugin-unused-imports": "2.0.0",
		"figlet": "1.5.2",
		"husky": "8.0.1",
		"lint-staged": "13.0.3",
		"npm-run-all": "4.1.5",
		"open-cli": "7.1.0",
		"prettier": "2.7.1",
		"ts-loader": "9.4.2",
		"typescript": "4.8.4",
		"webpack": "5.74.0",
		"webpack-merge": "5.8.0"
	},
	"peerDependencies": {
		"@types/inquirer": "9.0.3"
	}
}
